//if you don't know what a parameter does, visit:
//https://developer.valvesoftware.com/wiki/$phong
"VertexLitGeneric"
{
    "$basetexture" "models\weapons\arc9\scpsl\com18\com18_main_rgb"
    "$bumpmap" "models\weapons\arc9\scpsl\com18\com18_main_n"
    "$phongexponenttexture" "models\weapons\arc9\scpsl\com18\com18_main_e"

    "$color2" "[.9 .9 .9]"

    "$phong" "1"
    "$phongboost" "1"
    "$phongalbedotint" "1"
    "$phongalbedoboost" "1"                                //toy around with this
    "$phongfresnelranges" "[1 1.5 1]"

    //rimlight doesn't properly show in hlmv, make sure you're changing these values in game
    "$rimlight" "1"
    "$rimlightexponent" "100"
    "$rimmask" "1"
    "$rimlightboost" "1"
       

    "$normalmapalphaenvmapmask" "1"
    "$envmapfresnel" "1"
    "$envmap" "env_cubemap"
    "$envmaptint" "[0.05 0.05 0.05]"
	
	
    $detailblendmode 4
    $detailblendfactor .7
    $detail "models/weapons/arc9/colors/black_detail"

   Proxies {
        CustomCamo {
            camoTexture $detail
            camoScale $detailscale
            blend $detailblendfactor
            color2 [.8 .8 .8]
        }
        "Arc9MapTint"
        {
            "specularity" "0.5"             //0.25, 0.5, 0.75, 1
            "min" "0"
            "max" "1"                       //change this if too bright in game
        }
    }
}